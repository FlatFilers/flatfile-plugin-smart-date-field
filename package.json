{
  "name": "@flatfile/plugin-smart-date-field",
  "version": "0.0.0",
  "description": "A plugin for accepting and formatting dates in Flatfile.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FlatFilers/flatfile-plugin-smart-date-field.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FlatFilers/flatfile-plugin-smart-date-field/issues"
  },
  "homepage": "https://github.com/FlatFilers/flatfile-plugin-smart-date-field#readme",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "lint": "TIMING=1 eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix",
    "clean": "rm -rf node_modules && rm -rf dist",
    "test": "jest --runInBand --detectOpenHandles --forceExit --passWithNoTests"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@flatfile/configure": "^0.5.19",
    "@types/jest": "^28.1.4",
    "@types/lodash": "^4.14.192",
    "@typescript-eslint/parser": "^5.57.0",
    "chrono-node": "^2.6.2",
    "date-fns-tz": "^2.0.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.2",
    "lodash": "^4.17.21",
    "prettier": "^2.8.7",
    "ts-jest": "^28.0.5",
    "tsup": "^6.7.0"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
